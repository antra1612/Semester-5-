          //$('#ex1').hide();
          }
        else 	$('#dwn-error1').html(response);
        //$('#hide-download-brochure').html(response);

      },
      error:function(){
          alert('Failed');
        $('#dwn-error1').html('Some error occured');
        return false;
        // alert('Failed');
      }
    });
   }else{
   	$('#dwn-error1').html('Please enter email address');
   	return false;
   }
}else{
  $('#dwn-error1').html('Please enter name');
  return false;
}
});
});
